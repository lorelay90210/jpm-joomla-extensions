<?xml version="1.0" encoding="utf-8"?>
<extension version="1.6" type="plugin" group="content">
	<name>Google Calendar Plugin</name>
	<author>Jan Maat</author>
	<creationDate>February, 2012</creationDate>
	<copyright>This extension in released under the GNU/GPL License -
		http://www.gnu.org/copyleft/gpl.html</copyright>
	<authorEmail>jenj.maat@gmail.com</authorEmail>
	<version>0.2</version>
	<description>PLG_CONTENT_GOOGLECALENDAR_DESCRIPTION</description>


	<files>
		<filename plugin="googlecalendar">googlecalendar.php</filename>
		<filename>index.html</filename>
		
	</files>
	<languages folder="language">
		<language tag="nl-NL">nl-NL/nl-NL.plg_content_googlecalendar.ini</language>
		<language tag="nl-NL">nl-NL/nl-NL.plg_content_googlecalendar.sys.ini</language>		
	</languages>
	<config>
		<fields name="params">
			<fieldset name="basic">					
					<field name="code" type="textarea"  filter="raw" label="CODE" description="DESCR_CODE" rows="5" cols="40" />							
			</fieldset>		
		</fields>
	</config>
	
	
</extension>	
